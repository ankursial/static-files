<source xml:lang="en-US">Here's a JSON sample:</source>
<target xml:lang="en-XX">zz_Here's a JSON sample:_ZZ</target>
</trans-unit>
<trans-unit id="tu4" xml:space="preserve">
<source xml:lang="en-US">{
  "foo": "bar"
}
</source>
<target xml:lang="en-XX">zz_{
  "foo": "bar"
}
_ZZ</target>
</trans-unit>
</body>
</file>
</xliff>
